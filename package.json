{
  "name": "nas",
  "version": "0.0.14",
  "main": "./dist/index.js",
  "dependencies": {
    "@alicloud/fc2": "^2.2.1",
    "@alicloud/pop-core": "^1.7.10",
    "@serverless-devs/core": "^0.0.x",
    "async": "^3.2.0",
    "fs-extra": "^9.1.0",
    "ignore": "^5.1.8",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.20",
    "md5-file": "^5.0.0",
    "node-machine-id": "^1.1.12",
    "os-locale": "^5.0.0",
    "rimraf": "^3.0.2",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "14",
    "f2elint": "^0.4.4",
    "jest": "^26.4.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.14",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npx tsc -w -p tsconfig.json",
    "prebuild": "rimraf dist",
    "package-zip": "rm -rf ./dist/utils/fcResources/nas-server-*.zip && make -C ./src/utils/fcResources/ensure-nas-dir-exist package && make -C ./src/utils/fcResources/nas-server package",
    "build": "npx tsc -p tsconfig.json",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
